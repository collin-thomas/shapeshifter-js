{
  "name": "shapshifter-js",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/figlet": "^1.5.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "cp -R public/* dist &&  bun build ./src/app.js --outdir ./dist",
    "build:dev": "cp -R public/* dist &&  bun build ./src/app.js --outdir ./dist  --watch",
    "dev": "bun --hot index.js",
    "start": "bun run index.js"
  },
  "dependencies": {}
}
